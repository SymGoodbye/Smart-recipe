.container {
  display: flex;
  flex-direction: column;
}

.head_img {
  width: 100%;
  height: 320rpx;
  -webkit-filter: blur(20px);
}

.head {
  display: flex;
  flex-direction: column;
  position: absolute;
  margin-top: 60rpx;
  margin-left: 50rpx;
}

.head_title {
  font-size: 19px;
  color: #fff;
  font-weight: bold;
  letter-spacing: 2px;
}

.title_img {
  width: 40%;
  height: 225rpx;
  position: absolute;
  top: 160rpx;
  right: 30rpx;
  border-radius: 10%;
}

.icon {
  flex-direction: row;
  margin-top: 60rpx;
}

.icon_ {
  flex-direction: row;
}

.icon_img {
  margin-top: 1px;
  height: 14px;
  vertical-align: middle;
  width: 16px;
}

.icon_text {
  font-size: 24rpx;
  vertical-align: middle;
  margin-left: 10px;
}

.summary {
  display: flex;
  flex-direction: column;
  margin-left: 40rpx;
  margin-top: 50rpx;
  margin-right: 20px;
  color: #777;
}

/* .hr {
  margin-top: 45rpx;
  height: 1px;
  width: 100%;
  background-color: #d9d9d9;
} */

.tag {
  display: flex;
  flex-direction: row;
  margin-top: 50rpx;
}

.tag_img {
  height: 18px;
  vertical-align: middle;
  width: 18px;
}

.tag_text {
  font-size: 24rpx;
  vertical-align: middle;
  margin-left: 10px;
}

.tool {
  margin-top: 20rpx;
}

.circle-img {
  float: right;
  margin-right: 40rpx;
  vertical-align: middle;
}

.circle-img image {
  width: 90rpx;
  height: 90rpx;
}

.share-img {
  margin-left: 30rpx;
}

.horizon {
  width: 660rpx;
  height: 1px;
  background-color: #e5e5e5;
  vertical-align: middle;
  position: relative;
  top: 46rpx;
  margin: 0 auto;
  z-index: -99;
}

.list {
  display: flex;
  flex-direction: column;
  margin-left: 40rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.list_title {
  font-size: 35rpx;
  color: #111;
  font-weight: bold;
  letter-spacing: 2px;
}

.inner {
  margin-top: 30rpx;
  width: 660rpx;
}

.mname {
  text-align: left;
  font-size: 28rpx;
}

.amount {
  font-size: 28rpx;
  float: right;
}

.amount_innner {
  margin-left: 50rpx;
}
