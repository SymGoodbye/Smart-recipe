.container_ {
  display: flex;
  flex-direction: column;
  margin-top: 20rpx;
  margin-bottom: 40rpx;
  background-color: #fff;
  border-bottom: 1px solid #ededed;
  border-top: 1px solid #ededed;
  padding-bottom: 5px; /*下内边距*/
}

.title_{
  font-size: 34rpx;
  font-weight: 600;
  color: #333;
  margin-top: 10px;
  margin-bottom: 10px;
}

.title_img_ {
  margin-left: 16px;
  width: 100%;
  height: 340rpx;
  margin: auto 0;
  margin-bottom: 5px;
  border-radius: 2%;
}

.content_ {
  color: #666;
  font-size: 28rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
  letter-spacing: 2rpx;
  line-height: 40rpx;
}
